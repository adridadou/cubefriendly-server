#API
POST    /admin/source/load          @controllers.SourceProcessorRestController.loadForm
GET     /admin/source/pending       @controllers.SourceProcessorRestController.listPending
GET     /admin/source/header/:file  @controllers.SourceProcessorRestController.headerAction(file)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
